<div class="product12_image-wrapper"><img li-object:src="product.featured_image | image_url: width: 720" li-object:alt="product.featured_image.alt" alt="{{ product.featured_image.alt }}" src="{{ product.featured_image | image_url: width: 720 }}" loading="lazy" class="product12_image">
</div><script src="{{ 'liquify_custom.js' | asset_url }}" type="text/javascript"></script>
<div li-element="direct-add-to-cart" class="button is-tertiary is-small" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" @click="addToCart(true)"><div>Add to cart</div></div>
