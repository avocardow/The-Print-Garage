<div class="product12_image-wrapper"><img li-object:src="product.featured_image | image_url: width: 720" li-object:alt="product.featured_image.alt" alt="{{ product.featured_image.alt }}" src="{{ product.featured_image | image_url: width: 720 }}" loading="lazy" class="product12_image">
</div><div li-object:alt="section.settings.alt" li-object:src="section.settings.background | image_url: width: 720" class="product12_bg-image" alt="{{ section.settings.alt }}" src="{{ section.settings.background | image_url: width: 720 }}"></div><div class="product12_button-wrapper"><script src="{{ 'liquify_custom.js' | asset_url }}" type="text/javascript"></script>
<div li-element="direct-add-to-cart" class="button is-tertiary is-small" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" @click="addToCart($event, true)"><div>Add to cart</div></div></div>
